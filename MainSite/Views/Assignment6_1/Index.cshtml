@model StudentViewModel
@{
    ViewData["Title"] = "Assignment 6.1";
    bool showGrade = Model.AccessLevel >= 8;
    ViewBag.ShowGrade = showGrade;
}

<h2>Assignment 6.1</h2>

@if (Model.AccessLevel < 2)
{
    <p>You do not have a sufficient access level to view this data.</p>
}
else
{
    if (Model.AccessLevel >= 8)
    {
        <p><strong>Hello Admin</strong></p>
    }

    <ul>
        @foreach (var student in Model.Students)
        {
            @Html.Partial("_StudentRow", student)
        }
    </ul>
}
